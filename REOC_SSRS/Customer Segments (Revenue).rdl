<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily" MustUnderstand="df">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="REOC">
      <DataSourceReference>REOC</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>c0a02b02-a61b-43bd-8655-61754fb5e1db</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>REOC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    c.segment_name,
    c.gender,
    COUNT(DISTINCT f.customer_FK) AS CustomerCount,
    SUM(f.amount) AS TotalRevenue,
    AVG(c.income) AS AvgIncome
FROM dbo.vw_FactTransaction_Expanded f
JOIN dbo.DIM_CUSTOMER c ON f.customer_sk = c.customer_sk
WHERE f.fulldate BETWEEN @StartDate AND @EndDate
GROUP BY c.segment_name, c.gender
ORDER BY TotalRevenue DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="segment_name">
          <DataField>segment_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="gender">
          <DataField>gender</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustomerCount">
          <DataField>CustomerCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalRevenue">
          <DataField>TotalRevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AvgIncome">
          <DataField>AvgIncome</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
