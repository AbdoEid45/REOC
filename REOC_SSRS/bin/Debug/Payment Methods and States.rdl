<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily" MustUnderstand="df">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="REOC">
      <DataSourceReference>REOC</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>7eec15dd-73fd-4ac1-afd0-4e696d25c11d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>REOC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    pm.type AS PaymentMethod,
    ps.PaymentState_Type,
    COUNT(*) AS TxnCount,
    SUM(f.amount) AS TotalAmount
FROM dbo.vw_FactTransaction_Expanded f
LEFT JOIN dbo.[DIM_Payment method] pm ON f.payment_method_number = pm.payment_method_number
LEFT JOIN dbo.Dim_PaymentState ps ON f.[payment state_fk] = ps.PaymentState_ID
WHERE f.fulldate BETWEEN @StartDate AND @EndDate
GROUP BY pm.type, ps.PaymentState_Type
ORDER BY pm.type, ps.PaymentState_Type;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PaymentMethod">
          <DataField>PaymentMethod</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PaymentState_Type">
          <DataField>PaymentState_Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TxnCount">
          <DataField>TxnCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalAmount">
          <DataField>TotalAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
