<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily" MustUnderstand="df">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="REOC">
      <DataSourceReference>REOC</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e8954de3-0bf8-44fe-bc7b-537bb37b0bd1</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>REOC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH Monthly AS (
  SELECT
    CAST(DATEFROMPARTS(d.year, d.month, 1) AS date) AS MonthStart,
    SUM(f.amount) AS Revenue
  FROM dbo.vw_FactTransaction_Expanded f
  JOIN dbo.DIM_TABLE_DATE d ON f.fulldate = d.fulldate
  WHERE d.fulldate BETWEEN @StartDate AND @EndDate
  GROUP BY d.year, d.month
),
YOY AS (
  SELECT
    m.MonthStart,
    m.Revenue,
    LAG(m.Revenue, 12) OVER (ORDER BY m.MonthStart) AS RevenuePY
  FROM Monthly m
)
SELECT
  MonthStart,
  Revenue,
  RevenuePY,
  CASE WHEN RevenuePY &gt; 0 THEN (Revenue - RevenuePY) / RevenuePY ELSE NULL END AS YoYPct
FROM YOY
ORDER BY MonthStart;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
